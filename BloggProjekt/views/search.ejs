<div class="author">
  <h1 class="authorHeading">Best results</h1>
</div>

<section class="articles">
  <ul class="articleUl">
    <% data.forEach(post => { %>
      <li>
        <a href="/post/<%= post._id %>">
          <span><%= post.title %></span>
          <span class="articleListDate"><%= post.createdAt.toDateString() %></span>
        </a>
      </li>
    <% }) %>
  </ul>
</section>